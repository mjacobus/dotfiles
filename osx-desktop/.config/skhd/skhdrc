# .config/skhd/skhdrc
# brew install koekeishiya/formulae/skhd
# skhd --start-service
# skhd --restart-service

modifiers + key : command

cmd - return : open -na "iTerm.app"
cmd - s : open -na "Slack"
# cmd - b      : open -na "Google Chrome"
cmd - w : yabai -m window --close
cmd - b : yabai -m window --close
